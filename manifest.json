{
  "manifest_version": 2,
  "name": "__MSG_vkContest__",
  "version": "0.0.2",
  "description": "Random number generator",
  "icons": {
    "128": "images/icons/icon_128.png"
  },
  "content_scripts": [{
    "matches": ["http://m.vk.com/*", "https://m.vk.com/*"],
    "js": ["application.js"],
    "css": ["style.css"]
  }],
  "default_locale": "en",
  "permissions": ["http://m.vk.com/", "https://m.vk.com/"],
  "web_accessible_resources": [
    "images/*.png",
    "images/*.svg"
  ]
}